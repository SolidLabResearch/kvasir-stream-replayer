{
  "name": "kvasir-stream-replayer",
  "version": "1.0.0",
  "description": "TypeScript library for real-time streaming of RDF data from .nt files into Kvasir timeseries storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "demo": "ts-node src/demo.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc"
  },
  "keywords": [
    "kvasir",
    "rdf",
    "timeseries",
    "replayer",
    "typescript"
  ],
  "author": "Kush Bisen",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "n3": "^1.17.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/n3": "^1.26.0",
    "@types/progress": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  }
}
