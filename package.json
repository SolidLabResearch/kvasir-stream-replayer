{
  "name": "kvasir-stream-replayer",
  "version": "1.0.0",
  "description": "TypeScript library for real-time streaming of RDF data from .nt files into Kvasir timeseries storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "demo": "ts-node src/demo.ts",
    "example:subscription": "ts-node examples/graphql-subscription.ts",
    "example:polling": "ts-node examples/polling-subscription-example.ts",
    "example:sse": "ts-node examples/sse-subscription-example.ts",
    "example:demo": "ts-node examples/subscription-demo.ts",
    "example:integration": "ts-node examples/pub-sub-integration.ts",
    "example:comprehensive-latency": "ts-node examples/comprehensive-latency-analysis.ts",
    "example:producer": "ts-node examples/realtime-event-producer.ts",
    "example:subscriber": "ts-node examples/realtime-sse-subscriber.ts",
    "benchmark:sse-latency": "ts-node src/latency-benchmark-sse.ts",
    "benchmark:sse-multiple": "ts-node src/run-multiple-benchmarks.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc"
  },
  "keywords": [
    "kvasir",
    "rdf",
    "timeseries",
    "replayer",
    "typescript"
  ],
  "author": "Kush Bisen",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "eventsource": "^4.0.0",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.3",
    "n3": "^1.17.0",
    "progress": "^2.0.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/jest": "^29.0.0",
    "@types/n3": "^1.26.0",
    "@types/progress": "^2.0.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  }
}
